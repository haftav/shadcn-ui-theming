{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js --watch ./server/**/*.js",
    "dev:remix": "remix watch",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:shell": "docker exec -it classical_db /bin/bash",
    "db:migrate:up": "npx dbmate up && npx kysely-codegen",
    "db:migrate:up:prod": "npx dbmate up && npx kysely-codegen",
    "db:migrate:down": "npx dbmate down",
    "db:reset": "npx dbmate drop && npm run db:migrate:up",
    "format": "npx prettier --write ./app",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./server.js",
    "types": "tsc"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.2",
    "@remix-run/express": "^1.14.0",
    "@remix-run/node": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "axios": "^1.3.3",
    "better-sqlite3": "^8.1.0",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "jsdom": "^21.1.0",
    "keyv": "^4.5.2",
    "kysely": "^0.24.2",
    "morgan": "^1.10.0",
    "pg": "^8.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "remix-auth": "^3.4.0",
    "remix-auth-socials": "^2.0.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@types/better-sqlite3": "^7.6.3",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "dbmate": "^2.2.0",
    "eslint": "^8.27.0",
    "kysely-codegen": "^0.10.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
